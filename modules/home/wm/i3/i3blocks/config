full_text=
separator=false

[wintitle]
command=python3 ~/.config/i3blocks/workspace-info
markup=pango
interval=1

[separator]

[keymap]
command=~/.config/i3blocks/xkb-layout "@bg2@" "@fg1@"
interval=persist
markup=pango

[volume]
command=~/.config/i3blocks/volume
interval=1
markup=pango

[separator]

[datetime]
command=printf "<span foreground=\"#@bg2@\">î‚¶<span background=\"#@bg2@\" color=\"#@fg1@\">ðŸ•’ %s</span>î‚´</span>\n" "$(date '+%H:%M %a %d %b')"
interval=60
markup=pango

[notify]
command=i3-notification-status i3blocks '#@a1@' '#@g6@' '#@g9@' '#@g4@' '#@bg2@' '#@fg1@'
interval=1
markup=pango

[separator]

